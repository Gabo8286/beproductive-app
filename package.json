{
  "name": "beproductive-v2-spark-bloom-flow",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build && node scripts/bundle-analyzer.js",
    "build:stats": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:report": "npx playwright show-report",
    "test:performance": "npx playwright test --config=tests/performance/playwright.config.ts",
    "test:performance:headed": "npx playwright test --config=tests/performance/playwright.config.ts --headed",
    "test:performance:report": "npx playwright show-report test-results/performance-report",
    "test:perf:vitals": "npx playwright test --config=tests/performance/playwright.config.ts tests/performance/web-vitals.spec.ts",
    "test:perf:load": "npx playwright test --config=tests/performance/playwright.config.ts tests/performance/load-testing.spec.ts",
    "test:perf:regression": "npx playwright test --config=tests/performance/playwright.config.ts tests/performance/regression-testing.spec.ts",
    "ai:validate": "node scripts/ai-system-validator.js",
    "ai:health": "node scripts/ai-system-validator.js",
    "bundle:analyze": "node scripts/bundle-analyzer.js",
    "bundle:visualize": "npx vite-bundle-analyzer",
    "quality:analyze": "node scripts/code-quality-analyzer.js",
    "quality:full": "npm run lint && npm run test:coverage && npm run quality:analyze",
    "test:orchestrate": "node scripts/test-orchestrator.js",
    "test:enterprise": "npm run test:orchestrate",
    "gates:check": "npm run quality:full && npm run test:enterprise && npm run bundle:analyze && npm run 5s:check",
    "gates:deploy": "npm run gates:check && npm run 5s:enforce && echo 'Ready for deployment!'",
    "perf:audit": "npx lighthouse-ci autorun",
    "size-limit": "npx size-limit",
    "5s:analyze": "node scripts/5s-agent.js",
    "5s:sort": "node scripts/5s-agent.js --phase=seiri",
    "5s:organize": "node scripts/5s-agent.js --phase=seiton",
    "5s:clean": "node scripts/5s-agent.js --phase=seiso",
    "5s:standardize": "node scripts/5s-agent.js --phase=seiketsu",
    "5s:sustain": "node scripts/5s-agent.js --phase=shitsuke",
    "5s:report": "node scripts/5s-agent.js --report-only --format=markdown",
    "5s:check": "node scripts/5s-agent.js --check-gates",
    "5s:full": "npm run 5s:analyze && npm run 5s:report",
    "5s:enforce": "node scripts/5s-agent.js --enforce",
    "5s:trends": "node scripts/5s-agent.js --trends --days=30",
    "5s:dry-run": "node scripts/5s-agent.js --dry-run",
    "env:validate": "node scripts/validate-env.js",
    "env:validate:all": "node scripts/validate-env.js --all",
    "env:validate:staging": "node scripts/validate-env.js staging",
    "env:validate:production": "node scripts/validate-env.js production",
    "db:validate": "node scripts/validate-database.js",
    "db:migrate": "npx supabase migration up",
    "db:reset": "npx supabase db reset",
    "db:diff": "npx supabase db diff",
    "assets:optimize": "node scripts/optimize-assets.js",
    "build:optimized": "npm run build && npm run assets:optimize",
    "cdn:config": "node scripts/optimize-assets.js --config-only",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "backup:setup": "node scripts/backup-recovery.js",
    "backup:database": "scripts/backup/backup-database.sh",
    "backup:test": "scripts/backup/test-recovery.sh",
    "production:validate:env": "npm run env:validate:production && npm run db:validate && npm run monitoring:setup",
    "production:ready": "npm run production:validate && npm run backup:setup",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "clean": "rm -rf dist node_modules/.cache",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "prepare": "husky",
    "validate:fmew": "node scripts/test-fmew.js",
    "fmew:test": "node scripts/test-fmew.js",
    "fmew:validate": "node scripts/validate-dependencies.js && node scripts/validate-imports.js && node scripts/validate-css-classes.js",
    "test:production-readiness": "node scripts/production-readiness-orchestrator.js",
    "test:production-readiness:parallel": "node scripts/production-readiness-orchestrator.js --parallel",
    "test:production-readiness:sequential": "node scripts/production-readiness-orchestrator.js --sequential",
    "test:production-readiness:fail-fast": "node scripts/production-readiness-orchestrator.js --fail-fast",
    "test:security-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=security-tests",
    "test:performance-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=performance-tests",
    "test:reliability-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=reliability-tests",
    "test:compliance-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=compliance-tests",
    "test:ux-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=ux-tests",
    "test:devops-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=devops-tests",
    "test:data-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=data-tests",
    "test:integration-suite": "npx playwright test --config=src/test/production-readiness/config/production-readiness.config.ts --project=integration-tests",
    "production:gate-check": "npm run test:production-readiness",
    "production:deploy-ready": "npm run production:gate-check && echo '‚úÖ PRODUCTION DEPLOYMENT APPROVED!'",
    "security:scan": "npm run test:security-suite",
    "security:vulnerability-check": "npm audit --audit-level=moderate && npx playwright test src/test/production-readiness/01-security/vulnerability-scanning.test.ts",
    "test:production-readiness:security": "npx playwright test src/test/production-readiness/01-security/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:performance": "npx playwright test src/test/production-readiness/02-scalability-performance/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:reliability": "npx playwright test src/test/production-readiness/03-reliability-availability/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:compliance": "npx playwright test src/test/production-readiness/04-compliance-legal/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:ux": "npx playwright test src/test/production-readiness/05-ux-usability/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:devops": "npx playwright test src/test/production-readiness/06-devops-maintainability/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:data": "npx playwright test src/test/production-readiness/07-data-management/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:integration": "npx playwright test src/test/production-readiness/08-integration-extensibility/ --config=src/test/production-readiness/config/production-readiness.config.ts",
    "test:production-readiness:critical": "npm run test:production-readiness:security && npm run test:production-readiness:performance && npm run test:production-readiness:reliability",
    "test:production-readiness:full": "npm run test:production-readiness:security && npm run test:production-readiness:performance && npm run test:production-readiness:reliability && npm run test:production-readiness:compliance && npm run test:production-readiness:ux && npm run test:production-readiness:devops && npm run test:production-readiness:data && npm run test:production-readiness:integration",
    "production:report": "node scripts/production-readiness-report.js",
    "production:check": "node scripts/production-readiness-report.js",
    "production:status": "npm run production:check",
    "production:gate": "npm run production:check && echo 'üöÄ Production deployment approved!' || (echo '‚ùå Production deployment blocked!' && exit 1)"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "npx tsc --noEmit --skipLibCheck",
      "node scripts/validate-imports.js",
      "node scripts/validate-css-classes.js"
