    "5s:sustain": "node scripts/5s-agent.js --phase=shitsuke",
    "5s:report": "node scripts/5s-agent.js --report-only --format=markdown",
    "5s:check": "node scripts/5s-agent.js --check-gates",
    "5s:full": "npm run 5s:analyze && npm run 5s:report",
    "5s:enforce": "node scripts/5s-agent.js --enforce",
    "5s:trends": "node scripts/5s-agent.js --trends --days=30",
    "5s:dry-run": "node scripts/5s-agent.js --dry-run",
    "env:validate": "node scripts/validate-env.js",
    "env:validate:all": "node scripts/validate-env.js --all",
    "env:validate:staging": "node scripts/validate-env.js staging",
    "env:validate:production": "node scripts/validate-env.js production",
    "db:validate": "node scripts/validate-database.js",
    "db:migrate": "npx supabase migration up",
    "db:reset": "npx supabase db reset",
    "db:diff": "npx supabase db diff",
    "assets:optimize": "node scripts/optimize-assets.js",
    "build:optimized": "npm run build && npm run assets:optimize",
    "cdn:config": "node scripts/optimize-assets.js --config-only",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "backup:setup": "node scripts/backup-recovery.js",
    "backup:database": "scripts/backup/backup-database.sh",
    "backup:test": "scripts/backup/test-recovery.sh",
    "production:validate": "npm run env:validate:production && npm run db:validate && npm run monitoring:setup",
    "production:ready": "npm run production:validate && npm run backup:setup"
