{
  "name": "beproductive-v2-spark-bloom-flow",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:restart": "node scripts/dev-restart.cjs",
    "dev:cleanup": "node scripts/dev-cleanup.cjs",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build && node scripts/bundle-analyzer.js",
    "build:stats": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "node scripts/test-runner.js all",
    "test:unit": "node scripts/test-runner.js unit",
    "test:watch": "node scripts/test-runner.js unit --watch",
    "test:coverage": "node scripts/test-runner.js unit --coverage",
    "test:e2e": "node scripts/test-runner.js e2e",
    "test:performance": "node scripts/test-runner.js performance",
    "test:ci": "node scripts/test-runner.js ci",
    "test:run": "node scripts/test-runner.js ci",
    "test:quick": "node scripts/test-runner.js quick",
    "test:security": "node scripts/test-runner.js security",
    "ai:validate": "node scripts/ai-system-validator.js",
    "ai:health": "node scripts/ai-system-validator.js",
    "bundle:analyze": "node scripts/bundle-analyzer.js",
    "bundle:visualize": "npx vite-bundle-analyzer",
    "quality:analyze": "node scripts/code-quality-analyzer.js",
    "quality:check": "node scripts/quality-gate.js",
    "test:production": "node scripts/test-runner.js production",
    "gates:check": "node scripts/quality-gate.js",
    "gates:deploy": "npm run gates:check && echo 'Ready for deployment!'",
    "5s:run": "node scripts/5s-agent.js",
    "5s:report": "node scripts/5s-agent.js --report-only --format=markdown",
    "5s:check": "node scripts/5s-agent.js --check-gates",
    "validate:env": "node scripts/validate-env.js",
    "validate:db": "node scripts/validate-database.js",
    "build:optimized": "npm run build && node scripts/optimize-assets.js",
    "backup:setup": "node scripts/backup-recovery.js",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "clean": "rm -rf dist node_modules/.cache",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "prepare": "husky",
    "dev:mobile": "node scripts/open-mobile-window.js",
    "dev:iphone": "node scripts/open-mobile-window.js iphone14",
    "dev:android": "node scripts/open-mobile-window.js pixel7",
    "ios": "./scripts/ios-dev.sh",
    "ios:clean": "./scripts/ios-dev.sh --clean",
    "ios:verbose": "./scripts/ios-dev.sh --verbose",
    "ios:sim": "./scripts/ios-dev.sh --simulator 'iPhone 15 Pro'",
    "ios:package": "./scripts/ios-dev.sh --package",
    "ios:list": "./scripts/ios-dev.sh --list-projects",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:dev:detached": "docker-compose --profile dev up --build -d",
    "docker:dev:restart": "docker-compose --profile dev down && docker-compose --profile dev up --build",
    "docker:dev:logs": "docker-compose --profile dev logs -f dev",
    "docker:dev:shell": "docker-compose --profile dev exec dev sh",
    "docker:dev:stop": "docker-compose --profile dev down",
    "docker:dev:clean": "docker-compose --profile dev down -v && docker system prune -f",
    "docker:build": "docker build --target production -t spark-bloom-flow:latest .",
    "docker:prod": "docker-compose up --build app",
    "production:ready": "npm run test:production && npm run production:check",
    "production:check": "node scripts/production-readiness-report.js",
    "db:connect": "node scripts/supabase-client.js test",
    "db:health": "node scripts/supabase-client.js health",
    "db:super-admins": "node scripts/supabase-client.js super-admins",
    "db:admin": "node scripts/super-admin-manager.js",
    "db:setup": "node scripts/super-admin-manager.js setup",
    "db:status": "node scripts/super-admin-manager.js status",
    "db:migrate": "node scripts/apply-migration.js apply",
    "db:verify": "node scripts/apply-migration.js verify",
    "db:migrate-manual": "node scripts/apply-migration.js manual",
    "db:admin-client": "node scripts/supabase-admin.js",
    "db:admin-test": "node scripts/supabase-admin.js test",
    "db:admin-info": "node scripts/supabase-admin.js info",
    "db:migrate-core": "node scripts/migration-manager.js apply-core",
    "db:migrate-verify": "node scripts/migration-manager.js verify"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "npx tsc --noEmit --skipLibCheck",
      "node scripts/validate-imports.js",
      "node scripts/validate-css-classes.js"
    ],
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "package.json": [
      "node scripts/validate-dependencies.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@axe-core/react": "^4.10.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
